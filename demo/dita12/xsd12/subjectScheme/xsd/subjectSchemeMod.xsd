          <xs:attributeGroup ref="subjectHead.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="subjectHead.attlist">
    <xs:attributeGroup ref="subjectHead.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/subjectHead "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Subject Heading Metadata -->
  <xs:complexType name="subjectHeadMeta.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="navtitle"/>
      <xs:group minOccurs="0" ref="shortdesc"/>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="subjectHeadMeta.attributes">
    <xs:attribute name="lockmeta">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="no"/>
          <xs:enumeration value="yes"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="subjectHeadMeta">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="subjectHeadMeta.content">
          <xs:attributeGroup ref="subjectHeadMeta.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="subjectHeadMeta.attlist">
    <xs:attributeGroup ref="subjectHeadMeta.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicmeta subjectScheme/subjectHeadMeta "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Enumeration definition -->
  <xs:complexType name="enumerationdef.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="elementdef"/>
      <xs:group ref="attributedef"/>
      <xs:group maxOccurs="unbounded" ref="subjectdef"/>
      <xs:group minOccurs="0" ref="defaultSubject"/>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="data.elements.incl"/>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="enumerationdef.attributes">
    <xs:attributeGroup ref="id-atts"/>
    <xs:attribute name="outputclass"/>
    <xs:attribute name="status">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="changed"/>
          <xs:enumeration value="deleted"/>
          <xs:enumeration value="new"/>
          <xs:enumeration value="unchanged"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="base"/>
  </xs:attributeGroup>
  <xs:element name="enumerationdef">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="enumerationdef.content">
          <xs:attributeGroup ref="enumerationdef.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
