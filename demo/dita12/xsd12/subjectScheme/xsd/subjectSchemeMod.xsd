<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/">
  <xs:import namespace="http://dita.oasis-open.org/architecture/2005/" schemaLocation="ditaarch.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  
  <!-- ============================================================= -->
  <!-- ELEMENT DECLARATIONS -->
  <!-- ============================================================= -->
  <!-- LONG NAME: Subject Scheme Map -->
  <xs:complexType name="subjectScheme.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="title"/>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="anchor"/>
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="enumerationdef"/>
        <xs:group ref="hasInstance"/>
        <xs:group ref="hasKind"/>
        <xs:group ref="hasNarrower"/>
        <xs:group ref="hasPart"/>
        <xs:group ref="hasRelated"/>
        <xs:group ref="navref"/>
        <xs:group ref="relatedSubjects"/>
        <xs:group ref="reltable"/>
        <xs:group ref="schemeref"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="subjectRelTable"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="subjectScheme.attributes">
    <xs:attribute name="id" type="xs:ID"/>
    <xs:attributeGroup ref="conref-atts"/>
    <xs:attribute name="anchorref"/>
    <xs:attribute name="outputclass"/>
    <xs:attributeGroup ref="localization-atts"/>
    <xs:attributeGroup ref="topicref-atts-no-toc"/>
    <xs:attributeGroup ref="select-atts"/>
  </xs:attributeGroup>
  
  <xs:element name="subjectScheme">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="subjectScheme.content">
          <xs:attributeGroup ref="subjectScheme.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="subjectScheme.attlist">
    <xs:attributeGroup ref="subjectScheme.attributes"/>
    <xs:attribute ref="ditaarch:DITAArchVersion"/>
    <xs:attributeGroup ref="domains-att"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/map subjectScheme/subjectScheme "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Scheme reference -->
  <xs:complexType name="schemeref.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="data.elements.incl"/>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="schemeref.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="query"/>
    <xs:attribute name="type" default="scheme"/>
    <xs:attribute name="format" default="ditamap"/>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="schemeref">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="schemeref.content">
          <xs:attributeGroup ref="schemeref.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="schemeref.attlist">
    <xs:attributeGroup ref="schemeref.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/schemeref "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Has Narrower Relationship -->
  <xs:complexType name="hasNarrower.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="hasNarrower.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="scope">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="local"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format"/>
    <xs:attribute name="type"/>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="hasNarrower">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="hasNarrower.content">
          <xs:attributeGroup ref="hasNarrower.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="hasNarrower.attlist">
    <xs:attributeGroup ref="hasNarrower.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/hasNarrower "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Has Kind Relationship -->
  <xs:complexType name="hasKind.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="hasKind.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="scope">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="local"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format"/>
    <xs:attribute name="type"/>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="hasKind">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="hasKind.content">
          <xs:attributeGroup ref="hasKind.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="hasKind.attlist">
    <xs:attributeGroup ref="hasKind.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/hasKind "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Has Part Relationship -->
  <xs:complexType name="hasPart.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="hasPart.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="scope">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="local"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format"/>
    <xs:attribute name="type"/>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="hasPart">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="hasPart.content">
          <xs:attributeGroup ref="hasPart.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="hasPart.attlist">
    <xs:attributeGroup ref="hasPart.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/hasPart "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Has Instance Relationship -->
  <xs:complexType name="hasInstance.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="hasInstance.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="scope">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="local"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format"/>
    <xs:attribute name="type"/>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="hasInstance">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="hasInstance.content">
          <xs:attributeGroup ref="hasInstance.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="hasInstance.attlist">
    <xs:attributeGroup ref="hasInstance.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/hasInstance "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Has Related Relationship -->
  <xs:complexType name="hasRelated.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="hasRelated.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="collection-type" default="choice">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="choice"/>
          <xs:enumeration value="sequence"/>
          <xs:enumeration value="unordered"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="scope">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="local"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format"/>
    <xs:attribute name="type"/>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="hasRelated">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="hasRelated.content">
          <xs:attributeGroup ref="hasRelated.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="hasRelated.attlist">
    <xs:attributeGroup ref="hasRelated.attributes"/>
    <xs:attributeGroup ref="global-atts"/>
    <xs:attribute name="class" default="- map/topicref subjectScheme/hasRelated "/>
  </xs:attributeGroup>
  <!-- LONG NAME: Subject definition -->
  <xs:complexType name="subjectdef.content">
    <xs:sequence>
      <xs:group minOccurs="0" ref="topicmeta"/>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="data.elements.incl"/>
        <xs:group ref="hasInstance"/>
        <xs:group ref="hasKind"/>
        <xs:group ref="hasNarrower"/>
        <xs:group ref="hasPart"/>
        <xs:group ref="hasRelated"/>
        <xs:group ref="subjectdef"/>
        <xs:group ref="subjectHead"/>
        <xs:group ref="topicref"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="subjectdef.attributes">
    <xs:attribute name="navtitle"/>
    <xs:attribute name="href"/>
    <xs:attribute name="keyref"/>
    <xs:attribute name="keys"/>
    <xs:attribute name="query"/>
    <xs:attribute name="copy-to"/>
    <xs:attribute name="outputclass"/>
    <xs:attribute name="collection-type">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="choice"/>
          <xs:enumeration value="family"/>
          <xs:enumeration value="sequence"/>
          <xs:enumeration value="unordered"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="type"/>
    <xs:attribute name="scope">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="external"/>
          <xs:enumeration value="local"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="locktitle">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="no"/>
          <xs:enumeration value="yes"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format"/>
    <xs:attribute name="linking">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="none"/>
          <xs:enumeration value="normal"/>
          <xs:enumeration value="sourceonly"/>
          <xs:enumeration value="targetonly"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="toc">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="no"/>
          <xs:enumeration value="yes"/>
          <xs:enumeration value="-dita-use-conref-target"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="univ-atts"/>
  </xs:attributeGroup>
  <xs:element name="subjectdef">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="subjectdef.content">
          <xs:attributeGroup ref="subjectdef.attlist"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="subjectdef.attlist">
    <xs:attributeGroup ref="subjectdef.attributes"/>
